  <link rel="apple-touch-icon" sizes="57x57" href="<%= digest_url("/favs/apple-icon-57x57.png") %>">
  <link rel="apple-touch-icon" sizes="60x60" href="<%= digest_url("/favs/apple-icon-60x60.png") %>">
