  <link rel="apple-touch-icon" sizes="114x114" href="<%= digest_url("/favs/apple-icon-114x114.png") %>">
  <link rel="apple-touch-icon" sizes="120x120" href="<%= digest_url("/favs/apple-icon-120x120.png") %>">
  <link rel="apple-touch-icon" sizes="144x144" href="<%= digest_url("/favs/apple-icon-144x144.png") %>">
